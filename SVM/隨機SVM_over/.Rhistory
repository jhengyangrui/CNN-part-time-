require(e1071)
train <- read.csv("train.csv")
test <- read.csv("test.csv")
head(train)
str(train)
svmall <- svm(YN ~ ., data = train)
summary(svmall)
train.pred = predict(svmall, train)
test.pred = predict(svmall, test)
table(real=test$YN, predict=test.pred)
confus.matrix = table(real=test$YN, predict=test.pred)
sum(diag(confus.matrix))/sum(confus.matrix)
acc = sum(diag(confus.matrix))/sum(confus.matrix)
